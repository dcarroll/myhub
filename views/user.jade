extends layout

block content
  h1= title
  p Welcome to #{title}
  ul
    each activity in activities
      li= activity.label + " (" + activity.id + ")" 
        button(onclick='mute()') Mute
        button(onclick='start()') Start
        button(onclick='stop()') Stop
        button(onclick='volumeUp()') Volume Up
        button(onclick='volumeDown()') Volume Down
        
